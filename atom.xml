---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>PIntron</title>
 <link href="http://AlgoLab.github.com/PIntron/atom.xml" rel="self"/>
 <link href="http://AlgoLab.github.com/PIntron/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://AlgoLab.github.com/PIntron/</id>
 <author>
   <name>Yuri Pirola</name>
   <email>yuri.pirola@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://AlgoLab.github.com/PIntron{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://AlgoLab.github.com/PIntron{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
